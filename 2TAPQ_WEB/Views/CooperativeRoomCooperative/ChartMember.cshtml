@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutCooperative.cshtml";
}
<div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">
    <div class="row">
        <ol class="breadcrumb">
            <li>
                <a href="#">
                    <svg class="glyph stroked home">
                        <use xlink:href="#stroked-home"></use>
                    </svg>
                </a>
            </li>
            <li class="active">Danh sách sản xuất</li>
        </ol>
    </div>
    <!--/.row-->

    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Danh sách sản xuất</h1>
        </div>
    </div>
    <!--/.row-->

    <div class="alert alert-success">Thay Đổi Trạng Trái Thành Công !</div>
    <div class="row">

        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-body">
                    <div id="toolbar" class="select_area">
                        <select name="" class="form-control" onmousedown="if(this.options.length>2){this.size=2;}"
                                onchange='this.size=0;'>
                            <option value="1">Diện Tích</option>
                            <option value="2">Số Lượng Ao</option>
                            <option value="3">Sản Lượng</option>
                            <option value="4"></option>



                        </select>
                    </div>
                    <div id="toolbar" class="select_area" style="  position: relative; margin-left: 4px;">

                        <a class="btn btn-primary">

                            <i class="glyphicon glyphicon-refresh"></i> Load
                        </a>

                    </div>





                    <div class="col-md-9">

                        <div class="graphbox">

                            <div class="box">
                                <canvas id="earning"></canvas>
                        
                            </div>




                        </div>

                    </div>

                    <div class="col-md-3">

                        <ul class="list-group list-group">
                            <li class="list-group-item" id="areaChart"><a <a asp-action="CooperativeRoomCooperative">Ao</a></li>
                            <li class="list-group-item" id="accountChart" ><a asp-action="ChartMember">Member</a></li>
                            <li class="list-group-item"><a href="">Third item</a></li>
                        </ul>
                    </div>
                </div>



            </div>
        </div>
    </div>
    <!--/.row-->
</div>
<script src="~/js/jquery-1.11.1.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/chart.js@3.9.1/dist/chart.min.js"></script>
<script src="~/js/mychartMember.js"></script>